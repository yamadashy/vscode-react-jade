{
    "name": "vscode-react-jade",
    "displayName": "vscode-react-jade",
    "description": "Syntax highlighting for jade (react-jade-transformer)",
    "version": "0.0.3",
    "publisher": "yamadashy",
    "icon": "logo.png",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/yamadashy/vscode-react-jade.git"
    },
    "engines": {
        "vscode": "*"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "grammars": [
            {
                "scopeName": "react.jade",
                "path": "./syntaxes/react-jade.json",
                "injectTo": [
                    "source.js",
                    "source.ts",
                    "source.jsx",
                    "source.js.jsx",
                    "source.tsx"
                ],
                "embeddedLanguages": {
                    "react.jade": "jade"
                }
            }
        ]
    }
}
